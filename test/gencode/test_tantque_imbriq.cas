-- Test d'une boucle TantQue imbriqu√©e dans une boucle TantQue

program
    i : integer
    j : integer
begin
    i := 0;
    j := 0;

    while i < 5 do
        while j < 5 do
            write(i, " + , j, " = ", i+j);
            new_line;

            i := i+1;
            j := j+1;
        end;
    end;
end.
